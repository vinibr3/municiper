<div class="modal" tabindex="-1" id=<%= "edit-resident-#{resident.id}-modal"%>>
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title"><%= t('.new_resident') %></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body" id="edit-resident-form-wrapper">
        <%= render partial: 'form', locals: {  resident: resident } %>
      </div>
    </div>
  </div>
</div>
